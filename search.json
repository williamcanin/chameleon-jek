---
---
[
  {% for post in site.categories.blog %}
    {
      "title"    : "{{ post.title | escape }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "post_number" : "{{ post.article }}",
      "url"     : "{% if site.baseurl %}{{ site.baseurl }}{% endif %}{{ post.url }}",
      "date"     : "{{ post.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
